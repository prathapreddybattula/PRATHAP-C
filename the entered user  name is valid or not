#include<iostream>
using namespace std;
int main()
{
    string a,b;
    cout<<"enter a user name:";
    cin>>a;
    cout<<"re enter a user name";
    cin>>b;
    if(a==b){
        cout<<"user name is valid";
    }
else{
    cout<<"user name is invalid";
}
}


output:
enter a user name prathap
re enter a user name prathap
user name is valid
