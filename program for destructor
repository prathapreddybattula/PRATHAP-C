#include<iostream>
using namespace std;
class employee
{
    public:
    employee()
    {
        cout<<"constructor invoked"<<endl;
    }
    ~employee()
    {
        cout<<"destructor invoked"<<endl;
        
    }
};
int main(void)
{
    employee e1;//creating an object of employee
    employee e2;//creating an object of employee
    return 0;
}

output:

constructor invoked
constructor invoked
destructor  invoked
destructor  invoked
